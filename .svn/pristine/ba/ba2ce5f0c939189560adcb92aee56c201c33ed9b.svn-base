public class StuHomeWork {
	
	private int stuHwId;
	private int optionId;
	private int stuId;
	private int nwId;

	public int getStuHwId() {
		return stuHwId;
	}

	public void setStuHwId(int stuHwId) {
		this.stuHwId = stuHwId;
	}

	public int getOptionId() {
		return optionId;
	}

	public void setOptionId(int optionId) {
		this.optionId = optionId;
	}

	public int getStuId() {
		return stuId;
	}

	public void setStuId(int stuId) {
		this.stuId = stuId;
	}

	public int getNwId() {
		return nwId;
	}

	public void setNwId(int nwId) {
		this.nwId = nwId;
	}

}
